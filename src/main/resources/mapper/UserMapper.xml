<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="travel.plan.data.mapper.UserMapper">
    <select id="getUserById" resultType="UserDTO">
        select * from test where id = #{testId}
    </select>

    <select id="selectAll" resultType="UserDTO">
        select * from test
    </select>

    <insert id="userJoin" keyProperty="USER_ID" parameterType="hashMap">
        insert into user(USER_NICK, USER_PWD, REG_DTM, REGR_ID)
         values(#{username}, #{password}, #{reg}, #{username})
    </insert>
</mapper>